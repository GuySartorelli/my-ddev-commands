{
    "require": {
      "php": "^8.1",
      "guysartorelli/ddev-wrapper": "dev-main",
      "symfony/console": "^6.0",
      "symfony/process": "^6.0",
      "symfony/dotenv": "^6.0",
      "symfony/filesystem": "^6.3",
      "knplabs/github-api": "^3.13",
      "symfony/http-client": "^6.4",
      "nyholm/psr7": "^1.8",
      "knplabs/packagist-api": "^2.0",
      "composer/semver": "^3.4",
      "gitonomy/gitlib": "^1.3"
    },
    "autoload": {
      "psr-4": {
        "GuySartorelli\\DdevPhpUtils\\": "src/"
      }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
